{
  "defaultEnvironment": "development",
  "environmentVariable": "ASSETER_ENV",
  "packageManager": "yarn",
  "providers": {
    "default": {
      "options": {
        "cache": "false",
        "sourceMap": "true",
        "uglify": false
      },
      "environment": {
        "production": {
          "cache": "false",
          "uglify": true
        },
        "test": {
          "sourceMap": "true",
          "uglify": false
        }
      },
      "type": "webpack"
    },
    "landing": {
      "options": {
        "cache": false,
        "sourceMap": "false"
      },
      "environment": {
        "special": {
          "cache": true,
          "uglify": "true"
        }
      },
      "type": "rollup"
    }
  }
}
