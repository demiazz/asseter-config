{
  "defaultEnvironment": "development",
  "environmentVariable": "ASSETER_ENV",
  "packageManager": "yarn",
  "providers": {
    "default": {
      "options": {
        "cache": false,
        "sourceMap": true
      },
      "environment": {
        "production": {
          "cache": false
        },
        "test": {
          "sourceMap": true
        }
      },
      "type": "webpack"
    },
    "mainLanding": {
      "options": {
        "sourceMap": false
      },
      "environment": {
        "special": {
          "uglify": true
        }
      },
      "type": "rollup"
    }
  }
}
